tic
clc;clear all;
load('/.../lowsic.mat');
load('/.../highiosst.mat');

clear t0;clear t;var = 'Z3'; mon = 7; %%%
period = [1950:2014]; periodave = [1981:2010];
dir=['...'];
file=[dir,'/Z_3levels_global_90S90N_19002014_1.nc';
latcesm=ncread(file,'lat');loncesm=ncread(file,'lon');level=ncread(file,'lev'); 
latrange = near1(latcesm,15):near1(latcesm,90);
for num=1:100; num
    file=[dir,'Z_3levels_global_90S90N_19002014_',num2str(num),'.nc']; 
    t = squeeze(ncread(file,var)); t = squeeze( t(:,latrange,find(level == [850]) ,: ) ); %% level 200 500 850
    t = reshape(t, size(t,1),size(t,2),12,115);
    t = ( t (:,:,mon,period-1900+1 ));%%%
    t0(:,:,:,:,num) = t;
end;
z0_cesm_clim=mean(mean(t0(:,:,:,periodave-1950+1,:),4),5);
z0_cesm = t0 - nanmean(t0(:,:,:,periodave-1950+1,:),4); clear t0;clear t;
latcesm=ncread(file,'lat');loncesm=ncread(file,'lon'); 
toc


pmme= squeeze(z0_cesm (:,:,1,:,:));mflag = 'July';
cc=1;clear pattern1;
for i=1:2014-1950+1;   
    for j=1:100;      
        if highsst(j,i)==1 && lowsic(j,i)==0;  
        pattern1(:,:,cc) = pmme (:,:,i,j); cc=cc+1; 
        end;
end;end;data1 = mean(pattern1,3);
cc=1;clear pattern2;
for i=1:2014-1950+1; 
    for j=1:100;      
        if highsst(j,i)==1 && lowsic(j,i)==1;
        pattern2(:,:,cc) = pmme (:,:,i,j); cc=cc+1; 
        end;
end;end;data2 = mean(pattern2,3); 
data   = data2-data1;


flag = 500;%%%
mflag = 'July';
alpha = 0.05;;

close all;figure(1);
set(gcf, 'position', [10 100 480 280]); 
handaxes1 = axes('Position', [0.05 0.25 0.9 0.7]);
set(gcf,'color','w');
hold on;

    loncesm=cat(1,-360+loncesm,loncesm); latcesm=latcesm( near1(latcesm,15):near1(latcesm,90)) ;
