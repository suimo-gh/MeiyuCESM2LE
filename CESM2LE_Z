    m_contourf(LON',LAT'+latmove,data0,[-30:1:30],'linestyle','none'); hold on;  
    if     flag==200; cx=[-9 9]*2; caxis(cx);c=colorbar('vertical','position',[0.87 0.25 0.015 0.7]);c.XTick=[-12:3:12]*2;      
